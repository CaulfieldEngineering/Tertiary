<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Bundle Identifier -->
    <key>CFBundleIdentifier</key>
    <string>com.@wCOMPANY_ID@.@wPLUGIN_NAME@</string>

    <!-- Application Name -->
    <key>CFBundleName</key>
    <string>@wPLUGIN_NAME@</string>

    <!-- Application Version -->
    <key>CFBundleVersion</key>
    <string>@wPROJECT_VERSION@</string>

    <key>CFBundleShortVersionString</key>
    <string>@wPROJECT_VERSION@</string>

    <!-- Display Name for Plugin Hosts -->
    <key>CFBundleDisplayName</key>
    <string>@wCOMPANY_NAME@</string>

    <!-- Plugin Information -->
    <key>CFBundleExecutable</key>
    <string>@wPLUGIN_NAME@</string>
    
    <key>CFBundlePackageType</key>
    <string>BNDL</string>

    <!-- Developer Information -->
    <key>NSHumanReadableCopyright</key>
    <string>Â© 2024 @wCOMPANY_NAME@</string>

    <!-- Minimum Supported macOS Version -->
    <key>LSMinimumSystemVersion</key>
    <string>10.12</string>

    <!-- Principal Class (for standalone apps, typically NSApplication) -->
    <key>NSPrincipalClass</key>
    <string>NSApplication</string>

    <!-- Plugin-specific Keys (if needed) -->
    <!-- Example VST3-specific entry (remove if not needed) -->
    <key>AUMainVersion</key>
    <integer>1</integer>
</dict>
</plist>
